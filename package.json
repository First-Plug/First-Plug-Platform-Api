{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate:tenant": "ts-node -r tsconfig-paths/register scripts/migrate-tenant.ts",
    "migrate:direct": "ts-node -r tsconfig-paths/register scripts/migrate-direct.ts",
    "rollback:migration": "ts-node -r tsconfig-paths/register scripts/rollback-migration.ts",
    "rollback:direct": "ts-node -r tsconfig-paths/register scripts/rollback-direct.ts",
    "cleanup:duplicates": "ts-node -r tsconfig-paths/register scripts/cleanup-duplicates.ts",
    "init:warehouses": "ts-node -r tsconfig-paths/register src/warehouses/scripts/run-initialization.ts",
    "init:warehouses:direct": "ts-node -r tsconfig-paths/register src/warehouses/scripts/direct-initialization.ts",
    "cleanup:warehouses": "ts-node -r tsconfig-paths/register src/warehouses/scripts/cleanup-warehouses.ts",
    "compare:countries": "ts-node -r tsconfig-paths/register src/warehouses/scripts/compare-countries.ts",
    "count:countries": "ts-node -r tsconfig-paths/register src/warehouses/scripts/count-countries.ts",
    "show:warehouse-metrics": "ts-node -r tsconfig-paths/register src/warehouses/scripts/update-metrics.ts",
    "test:warehouse-migration": "ts-node -r tsconfig-paths/register src/warehouses/scripts/test-migration.ts",
    "cleanup:offices": "ts-node -r tsconfig-paths/register scripts/cleanup-offices.ts",
    "test:script": "ts-node -r tsconfig-paths/register scripts/test-simple.ts",
    "migrate:countries": "ts-node -r tsconfig-paths/register src/scripts/migrate-countries-to-codes.ts",
    "test:migration": "ts-node -r tsconfig-paths/register src/scripts/test-migration.ts"
  },
  "dependencies": {
    "@anatine/zod-nestjs": "^2.0.8",
    "@anatine/zod-openapi": "^2.2.5",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^10.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.2.6",
    "@nestjs/websockets": "^10.2.6",
    "@slack/webhook": "^7.0.2",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "json2csv": "^6.0.0-alpha.2",
    "mongoose": "^8.3.4",
    "mongoose-delete": "^1.0.2",
    "nestjs-slack-webhook": "^10.5.0",
    "openapi3-ts": "^4.3.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "soft-delete-plugin-mongoose": "^1.0.17",
    "validator": "^13.12.0",
    "zod": "^3.23.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
